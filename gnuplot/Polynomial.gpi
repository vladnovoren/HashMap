#! /usr/bin/gnuplot -persist

set datafile separator ';'
set terminal png size 1200, 900
set output "../graphs/polynomial.png"

set xlabel "bucket number"
set ylabel "amount of elements in bucket"

set title "Polynomial hash" font "Helvetica Bold, 16"

plot "../gnuplot/Polynomial.csv" using 1:2 with lines title "polynomial hash"