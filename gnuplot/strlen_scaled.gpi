#! /usr/bin/gnuplot -persist

set datafile separator ';'
set terminal png size 1200, 900
set output "../graphs/strlen_scaled.png"

set xlabel "bucket number"
set ylabel "amount of elements in bucket"

set xrange [0:200]

set title "String length hash" font "Helvetica Bold, 16"

plot "../gnuplot/strlen.csv" using 1:2 with lines title "string length hash"