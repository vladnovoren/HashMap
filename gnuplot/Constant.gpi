#! /usr/bin/gnuplot -persist

set datafile separator ';'
set terminal png size 1200, 900
set output "../graphs/constant.png"

set xlabel "bucket number"
set ylabel "amount of elements in bucket"

set title "Constant hash" font "Helvetica Bold, 16"

plot "../gnuplot/Constant.csv" using 1:2 with lines title "constant hash"